{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "biometrics-route": {
        "ClusterId": "biometrics-cluster",
        "Match": {
          "Path": "api/biometrics/{**catch-all}"
        }
      },
      "default-route": {
        "ClusterId": "default-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "biometrics-cluster": {
        "Destinations": {
          "node1": {
            "Address": "https://localhost:7001"
          },
          "node2": {
            "Address": "https://localhost:7002"
          }
        },
        "LoadBalancingPolicy": "RoundRobin"
      },
      "default-cluster": {
        "Destinations": {
          "api-server": {
            "Address": "https://localhost:7001"
          }
        }
      }
    }
  }
}